meta {
  name: Read the List of Linked Issues
  type: http
  seq: 1
}

get {
  url: {{baseUrl}}/issues/:id/links/:issueLinkId/issues?fields=id,idReadable,summary
  body: none
  auth: inherit
}

params:query {
  fields: id,idReadable,summary
}

params:path {
  id: {{issue}}
  issueLinkId: {{issueLinkID}}
}

settings {
  encodeUrl: true
}
