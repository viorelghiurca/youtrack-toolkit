meta {
  name: Move Issue to Another Project
  type: http
  seq: 2
}

post {
  url: {{baseUrl}}/issues/:id/project?fields=id,name,shortName
  body: json
  auth: inherit
}

params:query {
  fields: id,name,shortName
}

params:path {
  id: {{issue}}
}

headers {
  Content-Type: application/json
}

body:json {
  {
      "id": "{{projectID}}"
  }
}

settings {
  encodeUrl: true
}
