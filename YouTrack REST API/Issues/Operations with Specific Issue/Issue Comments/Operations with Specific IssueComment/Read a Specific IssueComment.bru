meta {
  name: Read a Specific IssueComment
  type: http
  seq: 2
}

get {
  url: {{baseUrl}}/issues/:id/comments/:issueCommentId?fields=id,created,author(name,id),text
  body: none
  auth: inherit
}

params:query {
  fields: id,created,author(name,id),text
}

params:path {
  id: {{issue}}
  issueCommentId: {{issueCommentID}}
}

settings {
  encodeUrl: true
}
